{
  "@context": "https://bevyframe.islekcaganmert.me/ns/manifest",
  "app": {
    "name": "BevyFrame Test App",
    "short_name": "Test App",
    "orientation": "any",
    "version": "1.0.0",
    "package": "com.example.demo",
    "style": "hereus_ui_3_2",
    "icon": "/assets/favicon.png",
    "loginview": "/login.py",
    "shareview": "/share.py",
    "offlineview": "/offline.py",
    "accept_media": ["image/*", "video/*"],
    "database_filename": "test",
    "allow_multiple_instance": true,
    "shortcuts": {
      "/env.html": "View Environment",
      "/demo.html": "Greet",
      "/data.py": "Decentralized Data"
    },
    "cors": false,
    "routing": {
      "/user/<email>": "/profile.py"
    },
    "disable_features": [
      "UserContext"
    ]
  },
  "publishing": {
    "description": "An example application showcasing BevyFrame.",
    "screenshots": []
  },
  "accounts": {
    "default_network": "hereus.net",
    "permissions": []
  },
  "development": {
    "host": "0.0.0.0",
    "port": 3000,
    "debug": true
  },
  "production": {
    "web": {
      "host": "0.0.0.0",
      "port": 80
    },
    "ios": false,
    "android": {
      "api_level": [33, 35],
      "permissions": [],
      "features": [],
      "adaptive_icon": {
        "foreground": "",
        "background": ""
      }
    },
    "nt": false,
    "macos": false,
    "snap": false,
    "flatpak": false,
    "luos": false
  },
  "requirements": []
}